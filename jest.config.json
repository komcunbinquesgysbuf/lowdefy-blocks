{
  "moduleFileExtensions": ["js", "json", "jsx", "ts", "tsx", "node", "yaml", "css"],
  "transform": {
    "\\.yaml$": "yaml-jest",
    "\\.js?$": "babel-jest"
  },
  "moduleNameMapper": {
    "\\.(css|less)$": "<rootDir>/tests/__mocks__/styleMock.js"
  },
  "coveragePathIgnorePatterns": ["/tests/", "/demo/"]
}
